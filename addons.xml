<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.Baksy" name="Baksy Repository" version="1.0.1" provider-name="Wardoctor">
	<extension point="xbmc.addon.repository" name="Baksy Repository">
		<info compressed="false">https://raw.githubusercontent.com/TheWardoctor/Baksy/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/TheWardoctor/Baksy/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/TheWardoctor/Baksy/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Baksy Repository</summary>
		<description></description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.baksy" version="2.0.9" name=" Baksy IPTV" provider-name="Wardoctor">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Baksy IPTV</summary>
        <description>Baksy IPTV</description>
        <disclaimer>The author does not host or distribute any of the content displayed by this addon. The author does not have any affiliation with the content providers.</disclaimer>
        <language>en</language>
        <platform>all</platform>
        <source></source>
        <email></email>
    </extension>
</addon>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.baksy.guide" name="Baksy IPTV Guide"
version="1.0.8" provider-name="Wardoctor">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.dateutil" version="2.4.2" />
        <import addon="script.module.pil" />
        <import addon="script.module.beautifulsoup4" />
    </requires>
    <extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
    <extension point="xbmc.addon.metadata">
        <description lang="en">TV Guide especially for Baksy IPTV. Based on Fullscreen TV Guide with major contributions from: primaeval, iainmacleod, im85288, RayW1986, bluezed, andy, Zomboided, tycoo, kjb86.</description>
        <disclaimer lang="en">Based on the original TV Guide addon by twinther, FTV Guide and Midraal's fork.</disclaimer>
        <language></language>
        <platform>all</platform>
        <license>GPLv2</license>
        <forum></forum>
        <source></source>
        <website></website>
        <email></email>
        <news>
        </news>
    </extension>
</addon>
</addons> 
